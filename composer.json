{
    "name": "osbre/fabric-php",
    "keywords": [
        "fabric.js"
    ],
    "homepage": "https://github.com/osbre/fabric-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Ostap Bregin",
            "email": "osbre@protonmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0",
        "spatie/data-transfer-object": "^3.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.3",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Osbre\\Fabric\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Osbre\\Fabric\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
